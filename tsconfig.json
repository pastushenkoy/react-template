{
	"compilerOptions": {
		"jsx": "react",
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"skipLibCheck": true
	},
	"include": ["./src/**/*", "webpack.config.ts"],
	"exclude": ["node_modules", "src/**/*.spec.ts"]
}
